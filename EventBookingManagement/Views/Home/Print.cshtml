<link rel="stylesheet" href="/css/site.css" />
<body class="print">
	<div class="table">
		<h3 class="text-center  mb-4">Admin Lists</h3>
		<table class="table-bordered table table-hover">
			<thead class="text-white-50 bg-dark text-center">
		<tr>
			<th>Username</th>
			<th>Password</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var i in Model)
		{
			<tr>
				<td>@i.Username</td>
				<td>@i.Password</td>
			</tr>
		}
	</tbody>
</table>
</div>
</body>